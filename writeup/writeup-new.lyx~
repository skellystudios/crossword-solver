height_special "totalheight"
status open

\begin_layout Plain Layout
\begin_inset ERT
status open

\begin_layout Plain Layout


\backslash
Tree[.DEFINITION
\backslash
 "companion" [.ANAGRAM
\backslash
 ("shredded") "corset" ]]
\end_layout

\end_inset


\end_layout

\end_inset


\begin_inset Box Frameless
position "t"
